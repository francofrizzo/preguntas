    "npm-run-all": "^4.1.5",
    "rimraf": "^2.6.2",
    "sqlite": "^2.9.3",
    "typescript": "^2.9.2"
  },
  "devDependencies": {
    "@types/body-parser": "^1.17.0",
    "@types/express": "^4.17.0"
  },
  "repository": {
    "type": "git",
